<!-- -->
<robot name="xycar">

  <link name="base_link"/>
  <link name="imu_link"/>
  <link name="laser"/>
  
  <joint name="baselink_to_laser" type="fixed">
    <parent link="base_link"/>
    <child link="laser"/>
    <origin rpy="0 0 0" xyz="0.15 0.0 0.23"/>
    <axis xyz="0 0 1"/>
  </joint>
  
  <joint name="baselink_to_imu_link" type="fixed">
    <parent link="base_link"/>
    <child link="imu_link"/>
    <origin rpy="0 0 0" xyz="0.08 0.0 0.175"/>
    <axis xyz="0 0 1"/>
  </joint>

</robot>
